////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	COUNT(DISTINCT AccommodationChangeHistorySliceLast.Guest) AS Guest,
	AccommodationChangeHistorySliceLast.Guest.Организация AS Организация
INTO втСотрудникиКорея
FROM
	InformationRegister.AccommodationChangeHistory.SliceLast AS AccommodationChangeHistorySliceLast
WHERE
	AccommodationChangeHistorySliceLast.CheckOutDate &gt;= &amp;CheckOutDate
	AND AccommodationChangeHistorySliceLast.AccommodationStatus.SortCode = 10
	AND AccommodationChangeHistorySliceLast.Guest.Citizenship.Code IN (410, 398, 804, 860, 762, 498, 400)
	AND AccommodationChangeHistorySliceLast.Room.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)

GROUP BY
	AccommodationChangeHistorySliceLast.Guest.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	Расш1_СоставНомерногоФонда.Номер AS Номер,
	Расш1_СоставНомерногоФонда.НомернойФонд AS НомернойФонд
INTO втСосотавНомерногоФонда
FROM
	InformationRegister.Расш1_СоставНомерногоФонда AS Расш1_СоставНомерногоФонда
WHERE
	Расш1_СоставНомерногоФонда.Номер.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	Расш1_СоставНомерногоФонда.НомернойФонд AS Организация,
	Расш1_СоставНомерногоФонда.Номер.Ref AS Номер,
	CASE
		WHEN Расш1_СоставНомерногоФонда.Номер.RoomStatus = VALUE(Справочник.RoomStatuses.ПустаяСсылка)
			THEN FALSE
		ELSE TRUE
	END AS СтатусКомнатыРС
INTO втЗанятоИСвободно
FROM
	InformationRegister.Расш1_СоставНомерногоФонда AS Расш1_СоставНомерногоФонда
WHERE
	NOT Расш1_СоставНомерногоФонда.Номер.DeletionMark
	AND NOT Расш1_СоставНомерногоФонда.Номер.IsVirtual
	AND Расш1_СоставНомерногоФонда.Номер.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	COUNT(DISTINCT втЗанятоИСвободно.Номер) AS Номер,
	втЗанятоИСвободно.Организация AS Организация
INTO втЗанятыеКомнаты
FROM
	втЗанятоИСвободно AS втЗанятоИСвободно
WHERE
	втЗанятоИСвободно.СтатусКомнатыРС = FALSE

GROUP BY
	втЗанятоИСвободно.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	COUNT(DISTINCT втЗанятоИСвободно.Номер) AS Номер,
	втЗанятоИСвободно.Организация AS Организация
INTO втСвободныеКомнаты
FROM
	втЗанятоИСвободно AS втЗанятоИСвободно
WHERE
	втЗанятоИСвободно.СтатусКомнатыРС = TRUE

GROUP BY
	втЗанятоИСвободно.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	COUNT(DISTINCT втЗанятоИСвободно.Номер) AS Номер,
	втЗанятоИСвободно.Организация AS Организация
INTO втСвободныеКомнатыБезБлокировки
FROM
	втЗанятоИСвободно AS втЗанятоИСвободно
WHERE
	втЗанятоИСвободно.СтатусКомнатыРС = TRUE
	AND NOT втЗанятоИСвободно.Номер.БлокировкаРазмещения

GROUP BY
	втЗанятоИСвободно.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	ISNULL(Расш1_НомерныеФонды.Presentation, 0) AS ПредставлениеОрганизации,
	ISNULL(втДомаКвартиры.КоличествоДомов, 0) AS КоличествоДомов,
	ISNULL(втДомаКвартиры.КоличествоКвартир, 0) AS КоличествоКвартир,
	ISNULL(втВсегоЖильцов.Guest, 0) AS КоличествоЖильцов,
	ISNULL(втКоличествоОдиноких.Guest, 0) AS КоличествоОдиноких,
	ISNULL(втТолькоСемейные.Guest, 0) AS КоличествоСемейных,
	ISNULL(втИТР.Guest, 0) AS ИТР,
	ISNULL(втОПР.Guest, 0) AS ОПР,
	ISNULL(втЧленыСемьи.Guest, 0) AS ЧленыСемьи,
	ISNULL(втСотрудникиЕгипет.Guest, 0) AS СотрудниковЕгипет,
	ISNULL(втСотрудникиРоссия.Guest, 0) AS СотрудниковРоссия,
	ISNULL(втСотрудникиБелорусь.Guest, 0) AS СотрудниковБелорусь,
	ISNULL(втСотрудникиКорея.Guest, 0) AS СотрудниковКорея,
	ISNULL(втЗанятыеКомнаты.Номер, 0) AS КоличествоЗанятыхКомнат,
	ISNULL(втСвободныеКомнаты.Номер, 0) AS КоличествоСвободныхКомнат,
	ISNULL(втСвободныеКомнатыБезБлокировки.Номер, 0) AS КоличествоСвободныхКомнатБезБлокировки
FROM
	Catalog.Расш1_НомерныеФонды AS Расш1_НомерныеФонды
		LEFT JOIN втВсегоЖильцов AS втВсегоЖильцов
		ON Расш1_НомерныеФонды.Ref = втВсегоЖильцов.Организация
		LEFT JOIN втКоличествоОдиноких AS втКоличествоОдиноких
		ON Расш1_НомерныеФонды.Ref = втКоличествоОдиноких.Организация
		LEFT JOIN втТолькоСемейные AS втТолькоСемейные
		ON Расш1_НомерныеФонды.Ref = втТолькоСемейные.Организация
		LEFT JOIN втИТР AS втИТР
		ON Расш1_НомерныеФонды.Ref = втИТР.Организация
		LEFT JOIN втОПР AS втОПР
		ON Расш1_НомерныеФонды.Ref = втОПР.Организация
		LEFT JOIN втСотрудникиЕгипет AS втСотрудникиЕгипет
		ON Расш1_НомерныеФонды.Ref = втСотрудникиЕгипет.Организация
		LEFT JOIN втСотрудникиРоссия AS втСотрудникиРоссия
		ON Расш1_НомерныеФонды.Ref = втСотрудникиРоссия.Организация
		LEFT JOIN втСотрудникиБелорусь AS втСотрудникиБелорусь
		ON Расш1_НомерныеФонды.Ref = втСотрудникиБелорусь.Организация
		LEFT JOIN втСотрудникиКорея AS втСотрудникиКорея
		ON Расш1_НомерныеФонды.Ref = втСотрудникиКорея.Организация
		LEFT JOIN втЗанятыеКомнаты AS втЗанятыеКомнаты
		ON Расш1_НомерныеФонды.Ref = втЗанятыеКомнаты.Организация
		LEFT JOIN втДомаКвартиры AS втДомаКвартиры
		ON Расш1_НомерныеФонды.Ref = втДомаКвартиры.Организация
		LEFT JOIN втСвободныеКомнаты AS втСвободныеКомнаты
		ON Расш1_НомерныеФонды.Ref = втСвободныеКомнаты.Организация
		LEFT JOIN втЧленыСемьи AS втЧленыСемьи
		ON Расш1_НомерныеФонды.Ref = втЧленыСемьи.Организация
		INNER JOIN втСвободныеКомнатыБезБлокировки AS втСвободныеКомнатыБезБлокировки
		ON Расш1_НомерныеФонды.Ref = втСвободныеКомнатыБезБлокировки.Организация
WHERE
	NOT Расш1_НомерныеФонды.DeletionMark</query>
