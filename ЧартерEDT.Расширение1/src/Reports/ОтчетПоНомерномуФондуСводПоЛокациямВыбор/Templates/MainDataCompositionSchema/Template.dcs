		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED DISTINCT
	Расш1_СоставНомерногоФонда.НомернойФонд AS Организация,
	COUNT(DISTINCT Расш1_СоставНомерногоФонда.Номер.Parent) AS КоличествоДомов,
	COUNT(DISTINCT CAST((CAST(Расш1_СоставНомерногоФонда.Номер.Parent.Description AS STRING(5))) + (CAST(Расш1_СоставНомерногоФонда.Номер.НомерКвартиры AS STRING(10))) AS STRING(10))) AS КоличествоКвартир
INTO втДомаКвартиры
