WHERE
	Расш1_СоставНомерногоФонда.Номер.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)
	AND NOT Расш1_СоставНомерногоФонда.Номер.DeletionMark

GROUP BY
	Расш1_СоставНомерногоФонда.НомернойФонд
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	COUNT(AccommodationChangeHistorySliceLast.Guest) AS Guest,
	AccommodationChangeHistorySliceLast.Guest.Организация AS Организация
INTO втВсегоЖильцов
FROM
	InformationRegister.AccommodationChangeHistory.SliceLast AS AccommodationChangeHistorySliceLast
WHERE
	AccommodationChangeHistorySliceLast.CheckOutDate &gt;= &amp;CheckOutDate
	AND AccommodationChangeHistorySliceLast.AccommodationStatus.SortCode = 10
	AND AccommodationChangeHistorySliceLast.Room.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)

GROUP BY
	AccommodationChangeHistorySliceLast.Guest.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	COUNT(DISTINCT AccommodationChangeHistorySliceLast.Guest) AS Guest,
	AccommodationChangeHistorySliceLast.Guest.Организация AS Организация
INTO втКоличествоОдиноких
FROM
	InformationRegister.AccommodationChangeHistory.SliceLast AS AccommodationChangeHistorySliceLast
WHERE
	AccommodationChangeHistorySliceLast.CheckOutDate &gt;= &amp;CheckOutDate
	AND AccommodationChangeHistorySliceLast.AccommodationStatus.SortCode = 10
	AND AccommodationChangeHistorySliceLast.Guest.ТипПроживания = VALUE(Enum.Расш1_ТипПроживания.Single)
	AND AccommodationChangeHistorySliceLast.Room.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)

GROUP BY
	AccommodationChangeHistorySliceLast.Guest.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	COUNT(DISTINCT AccommodationChangeHistorySliceLast.Guest) AS Guest,
	AccommodationChangeHistorySliceLast.Guest.Организация AS Организация
INTO втТолькоСемейные
FROM
	InformationRegister.AccommodationChangeHistory.SliceLast AS AccommodationChangeHistorySliceLast
WHERE
	AccommodationChangeHistorySliceLast.CheckOutDate &gt;= &amp;CheckOutDate
	AND AccommodationChangeHistorySliceLast.AccommodationStatus.SortCode = 10
	AND AccommodationChangeHistorySliceLast.Guest.ТипПроживания = VALUE(Enum.Расш1_ТипПроживания.family)
	AND AccommodationChangeHistorySliceLast.Room.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)

GROUP BY
	AccommodationChangeHistorySliceLast.Guest.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	COUNT(DISTINCT AccommodationChangeHistorySliceLast.Guest) AS Guest,
	AccommodationChangeHistorySliceLast.Guest.Организация AS Организация
INTO втИТР
FROM
	InformationRegister.AccommodationChangeHistory.SliceLast AS AccommodationChangeHistorySliceLast
WHERE
	AccommodationChangeHistorySliceLast.CheckOutDate &gt;= &amp;CheckOutDate
	AND AccommodationChangeHistorySliceLast.AccommodationStatus.SortCode = 10
	AND (AccommodationChangeHistorySliceLast.Guest.КатегорияПроживающего.Order = 1
			OR AccommodationChangeHistorySliceLast.Guest.КатегорияПроживающего.Order = 6)
	AND AccommodationChangeHistorySliceLast.Room.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)

GROUP BY
	AccommodationChangeHistorySliceLast.Guest.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	COUNT(DISTINCT AccommodationChangeHistorySliceLast.Guest) AS Guest,
	AccommodationChangeHistorySliceLast.Guest.Организация AS Организация
INTO втОПР
FROM
	InformationRegister.AccommodationChangeHistory.SliceLast AS AccommodationChangeHistorySliceLast
WHERE
	AccommodationChangeHistorySliceLast.CheckOutDate &gt;= &amp;CheckOutDate
	AND AccommodationChangeHistorySliceLast.AccommodationStatus.SortCode = 10
	AND (AccommodationChangeHistorySliceLast.Guest.КатегорияПроживающего.Order = 3
			OR AccommodationChangeHistorySliceLast.Guest.КатегорияПроживающего.Order = 8)
	AND AccommodationChangeHistorySliceLast.Room.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)

GROUP BY
	AccommodationChangeHistorySliceLast.Guest.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	COUNT(DISTINCT AccommodationChangeHistorySliceLast.Guest) AS Guest,
	AccommodationChangeHistorySliceLast.Guest.Организация AS Организация
INTO втЧленыСемьи
FROM
	InformationRegister.AccommodationChangeHistory.SliceLast AS AccommodationChangeHistorySliceLast
WHERE
	AccommodationChangeHistorySliceLast.CheckOutDate &gt;= &amp;CheckOutDate
	AND AccommodationChangeHistorySliceLast.AccommodationStatus.SortCode = 10
	AND AccommodationChangeHistorySliceLast.Guest.КатегорияПроживающего.Order = 4
	AND AccommodationChangeHistorySliceLast.Room.ЛокацияРазмещения IN(&amp;ЛокацияРазмещения)

GROUP BY
	AccommodationChangeHistorySliceLast.Guest.Организация
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	COUNT(DISTINCT AccommodationChangeHistorySliceLast.Guest) AS Guest,
